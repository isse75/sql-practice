   "source": [
    "-- Revenue by seller. Total rev, total freight, number of items (min 50)\n",
    "\n",
    "SELECT seller_id,\n",
    "ROUND(sum(price), 2) as total_rev,\n",
    "ROUND(sum(freight_value), 2) as total_freight,\n",
    "count(*) as total_items\n",
    "FROM olist_order_items_dataset\n",
    "GROUP BY seller_id\n",
    "HAVING total_items >= 50\n",
    "\n"
   ]
  }
